<h1>Listing games</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Geo tl lat</th>
    <th>Geo tl lon</th>
    <th>Geo br lat</th>
    <th>Geo br lon</th>
    <th>Start</th>
    <th>Puppetmaster</th>
  </tr>

<% @games.each do |game| %>
  <tr>
    <td><%=h game.title %></td>
    <td><%=h game.geo_tl_lat %></td>
    <td><%=h game.geo_tl_lon %></td>
    <td><%=h game.geo_br_lat %></td>
    <td><%=h game.geo_br_lon %></td>
    <td><%=h game.start %></td>
    <td><%=h game.puppetmaster_id %></td>
    <td><%= link_to 'Show', game %></td>
    <td><%= link_to 'Edit', edit_game_path(game) %></td>
    <td><%= link_to 'Destroy', game, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New game', new_game_path %>