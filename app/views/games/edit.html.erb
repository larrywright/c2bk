<h1>Editing game</h1>

<% form_for(@game) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :geo_tl_lat %><br />
    <%= f.text_field :geo_tl_lat %>
  </p>
  <p>
    <%= f.label :geo_tl_lon %><br />
    <%= f.text_field :geo_tl_lon %>
  </p>
  <p>
    <%= f.label :geo_br_lat %><br />
    <%= f.text_field :geo_br_lat %>
  </p>
  <p>
    <%= f.label :geo_br_lon %><br />
    <%= f.text_field :geo_br_lon %>
  </p>
  <p>
    <%= f.label :start %><br />
    <%= f.datetime_select :start %>
  </p>
  <p>
    <%= f.label :puppetmaster_id %><br />
    <%= f.text_field :puppetmaster_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @game %> |
<%= link_to 'Back', games_path %>